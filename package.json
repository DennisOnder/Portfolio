{
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Personal Portfolio",
  "main": "index.js",
  "scripts": {
    "build": "browserify js/main.js -o js/script.js",
    "watch": "watchify js/main.js -o js/script.js -v",
    "scss": "node-sass --watch ./scss -o ./css",
    "start": "concurrently \"npm run scss\" \"npm run watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DennisOnder/portfolio.git"
  },
  "author": "Dennis Onder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DennisOnder/portfolio/issues"
  },
  "homepage": "https://github.com/DennisOnder/portfolio#readme",
  "dependencies": {
    "@dogstudio/highway": "^2.1.2",
    "browserify": "^16.2.3",
    "reading-time": "^1.1.3",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0"
  }
}
